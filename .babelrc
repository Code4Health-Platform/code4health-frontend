{
  "presets": ["es2015", "react", "stage-2"],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    ["module-resolver", {
      "alias": {
         "^@(atoms|molecules|templates)/(.*)$": "./app/src/components/\\1/\\2/\\2",
         "^@organisms$": "./app/src/components/organisms",
         "^@page/(.*)$": "./app/src/components/pages/\\1/\\1",
         "^@pages$": "./app/src/components/pages",
         "^@components/(.*)$": "./app/src/components/\\1",
         "^@(reducers|actions)/(.*)$": "./app/src/redux/\\1/\\2"
      }
    }]
  ]
}
